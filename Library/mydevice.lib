+        4.5,  54mA,
+        6.0,   0A,
+        7.0, -40mA
+        )
+     ) * TABLE( TEMP, -50, 1.3, 25, 1, 125, 0.7)
+   }
X1  A    DPWR HC_CLAMP
ROH DPWR DPWR_OH 100meg

G_OL  DGND_OL DGND 
+ VALUE = { 
+   TABLE( V(DPWR,DGND),
+     2.0, TABLE( V(DGND_OL,DGND), ; VCC=2
+         -1.0,  -16mA, 
+          0,     0,
+          0.4,   6mA,
+          0.8,   8.4mA,
+          1.2,   9.1mA,
+          2.5,   9.9mA
+         ),
+     4.5, TABLE( V(DGND_OL,DGND), ; VCC=4.5
+         -1.0,  -34mA, 
+          0,     0,
+          1.0,   34mA,
+          1.5,   44mA,
+          2.5,   53mA,
+          5.0,   56mA
+         ),
+     6.0, TABLE( V(DGND_OL,DGND), ; VCC=6.0
+         -1.0,  -44mA, 
+          0,     0,
+          1.0,   44mA,
+          2.0,   72mA,
+          3.5,   86mA,
+          6.5,   89mA
+         )
+    ) * TABLE( TEMP, -50, 1.3, 25, 1, 125, 0.7)
+   }
X2  DGND A    HC_CLAMP
ROL DGND DGND_OL 100MEG

N1  A DGND_OL DPWR_OH DIN74HC DGTLNET=D IO_HC_DTOA
