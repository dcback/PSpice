+   TABLE( V(DPWR,DGND),
+     2.0, TABLE( V(DPWR_OH,DGND), ; VCC=2
+       -1.0,  11.5mA,
+        0,    10.5mA,
+        0.8,  10.0mA,
+        1.4,   7.4mA,
